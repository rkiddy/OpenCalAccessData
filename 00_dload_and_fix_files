# CVR_LOBBY_DISCLOSURE_CD.TSV
#
$file = "CVR_LOBBY_DISCLOSURE_CD.TSV";

if (defined($allFiles) || defined($files{$file})) {

    $found_file = $files{$file};

    $out_file = $found_file."_FIXED";

    print "\nfixing ".$file."...";

    open F1, $found_file;
    open F2, "> ".$out_file;

    while (<F1>) {
        $line = $_;

        $line = &usual_fixes($line);

        @p = split '\t', $line;

        if (scalar(@p) > 52) {

           undef @next;

           for ($idx = 0; $idx < 43; $idx++) { push @next, $p[$idx]; }

           $len = scalar(@p) - 52 + 1;

           $mid = join ' ', splice(@p, 43, $len);
           $mid =~ s/  */ /g;

           push @next, $mid;

           for ($idx = scalar(@p) - 8; $idx < scalar(@p); $idx++) {
               push @next, $p[$idx];
           }

           @p = @next;
        }

        $line = join "\t", @p;

        print F2 $line;
    }

    close F1;
    close F2;

    &compare_results($found_file, $out_file);
}

