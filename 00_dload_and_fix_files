# LCCM_CD.TSV
#
$file = "LCCM_CD.TSV";

if (defined($allFiles) || defined($files{$file})) {

    $found_file = $files{$file};

    $out_file = $found_file."_FIXED";

    print "\nfixing ".$file."...";

    open F1, $found_file;
    open F2, "> ".$out_file;

    while (<F1>) {
        $line = $_;

        $line = &usual_fixes($line);

        if ($line =~ /^1676346\t0\t1\t/) {
            @p = split '\t', $line;
            @p = &remove_field(7, @p);
            @p = &remove_field(7, @p);
            $line = join "\t", @p;
        }

        print F2 $line;
    }

    close F1;
    close F2;

    &compare_results($found_file, $out_file);
}

