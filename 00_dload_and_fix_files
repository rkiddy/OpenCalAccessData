    &compare_results($found_file, $out_file);
}

# EXPN_CD.TSV
#
$file = "EXPN_CD.TSV";

if (defined($allFiles) || defined($files{$file})) {

    $found_file = $files{$file};

    $out_file = $found_file."_FIXED";

    print "\nfixing ".$file."...";

    open F1, $found_file;
    open F2, "> ".$out_file;

    $top = <F1>;
    print F2 $top;

    while (<F1>) {
        $line = $_;

        $line = &usual_fixes($line);

        @p = split '\t', $line;

        undef @next;

        if ($p[14] ne "" && $p[14] !~ /^[0-9]{1,2}\/[0-9]{1,2}\/[0-9]{4} /) {
            @p = &remove_field(7, @p);
            @p = &add_empty_field_at_end(@p);
            $line = join "\t", @p;
        }

        # Row of data chopped into two.
        #
        # Is it worth pulling a "put together 2 lines of x and y columns into one x+y-1 row" worth its own subroutine? We will see.
        #
        if ($line =~ /^1606358\t0\t8\t/ && scalar(@p) == 8) {
            $last = pop @p;
            $last =~ s/[\r\n]//g;
            push @p, $last;
            @nextP = split '\t', <F1>;
            shift @nextP;
            push @p, @nextP;
            $line = join "\t", @p;
        }

        print F2 $line;
    }

    close F1;
    close F2;

    &compare_results($found_file, $out_file);
}

