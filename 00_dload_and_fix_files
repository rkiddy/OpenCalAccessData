# CVR2_CAMPAIGN_DISCLOSURE_CD.TSV
#
$file = "CVR2_CAMPAIGN_DISCLOSURE_CD.TSV";

if (defined($allFiles) || defined($files{$file})) {

    $found_file = $files{$file};

    $out_file = $found_file."_FIXED";

    print "\nfixing ".$file."...";

    open F1, $found_file;
    open F2, "> ".$out_file;

    while (<F1>) {
        $line = $_;

        $line = &usual_fixes($line);

        @p = split '\t', $line;

        if ($p[13] eq "Marilyn Lyon Election Debt Committee" && $p[18] eq "c") {
            $p[18] = "CA";
            $line = join "\t", @p;
        }

        if ($p[17] eq "Fresno CA") {
            $p[17] = "Fresno";
            $p[18] = "CA";
            $line = join "\t", @p;
        }

        print F2 $line;
    }

    close F1;
    close F2;

    &compare_results($found_file, $out_file);
}

