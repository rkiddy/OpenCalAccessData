# TEXT_MEMO_CD.TSV
#
# cols: FILING_ID AMEND_ID LINE_ITEM REC_TYPE FORM_TYPE REF_NO TEXT4000
#
$file = "TEXT_MEMO_CD.TSV";

if (defined($allFiles) || defined($files{$file})) {

    $found_file = $files{$file};

    $out_file = $found_file."_FIXED";

    print "\nfixing ".$file."...";

    $cmd = option("tr")." '\\r' '\\n' < ".$found_file." | ".option("grep")." -v \'^\$\' | ".option("perl")." -p -e 's/\\n/\\r\\n/g' -";

    @lines = `$cmd`;

    open T, ">".$out_file;

    foreach (@lines) {

        $line = $_;

        $line = &usual_fixes($line);

        @p = split '\t', $line;

        if (scalar(@p) <= 7) {
            print T $line;
        } else {
            for ($idx = 0; $idx <= 5; $idx++) { print T $p[$idx]."\t"; }
            for ($idx = 6; $idx < scalar(@p); $idx++) { print T " ".$p[$idx]; }
        }
    }

    close $out_file;

    &compare_results($found_file, $out_file);
}

print "\n\nOk\n\n";

exit(0);
